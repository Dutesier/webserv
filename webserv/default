server {
    listen       8080;
    root         webserv/website;
    location /cgi {
      fastcgi_pass .py;
      root         webserv/website/cgi;
    }
    server_name  domain1.com;
}

server {
  listen localhost:8081;
  server_name server;
}

server {
  listen localhost:8081;
  error_page 405 webserv/website/404.html;
  server_name mg;
}